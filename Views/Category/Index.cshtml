@model IEnumerable<ProjectDAW.ViewModels.Category>

@{
    ViewBag.Title = "Index";
}

<ul>
    @foreach (var category in Model)
    {
        <li>@Html.ActionLink(category.Name, "Show", new { id = category.CategoryId })</li>
    }
</ul>

@Html.ActionLink("Create Category", "Create")
<h2>Index</h2>

